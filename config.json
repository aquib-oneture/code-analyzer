{
    "java": {
        "extensions": ["java"],
        "routine": "^\\s*(public|protected|private|static|\\s)*\\s+[a-zA-Z<>[\\]]+\\s+[a-zA-Z_][a-zA-Z0-9_]*\\s*\\(.*\\)\\s*(?=\\{)"
    },

    "js": {
        "extensions": ["js", "jsp"],
        "routine": "function\\s+([a-zA-Z_$][a-zA-Z0-9_$]*\\s*\\(.*?\\))"   
    },

    "web": {
        "extensions": ["js", "html", "css", "jsp", "asp"],
        "routine": "function\\s+([a-zA-Z_$][a-zA-Z0-9_$]*\\s*\\(.*?\\))"   
    },
    "python": {
        "extensions": ["py"],
        "routine": "def\\s+([a-zA-Z_][a-zA-Z0-9_]*\\s*\\(.*?\\))"
    }
}